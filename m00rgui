		image.Image = "http://www.roblox.com/asset/?id=18213993071"
		image.Active = true
	
		for i, c in pairs(game.Players:GetChildren()) do
			local jumpscareClone = jumpscareGUI:Clone()
			jumpscareClone.Parent = c.PlayerGui
		end
		jumpscareSound:Play()
		wait(14)
		jumpscareSound:Destroy()
		for i, c in pairs(game.Players:GetChildren()) do
			c.PlayerGui.Jumpscare:Destroy()
		end
	end)
end
coroutine.wrap(YJXMUU_fake_script)()
local function UPCT_fake_script() -- decalspam_2.LocalScript 
	local script = Instance.new('LocalScript', decalspam_2)

	script.Parent.MouseButton1Click:Connect(function()
		local s = Instance.new("Sky")
		s.Name = "Sky"
		s.Parent = game.Lighting
		local skyboxID = 16000894791
		s.SkyboxBk = "http://www.roblox.com/asset/?id="..skyboxID
		s.SkyboxDn = "http://www.roblox.com/asset/?id="..skyboxID
		s.SkyboxFt = "http://www.roblox.com/asset/?id="..skyboxID
		s.SkyboxLf = "http://www.roblox.com/asset/?id="..skyboxID
		s.SkyboxRt = "http://www.roblox.com/asset/?id="..skyboxID
		s.SkyboxUp = "http://www.roblox.com/asset/?id="..skyboxID
		game.Lighting.TimeOfDay = 12
	
		decalID = 18213997018
		function exPro(root)
			for _, v in pairs(root:GetChildren()) do
				if v:IsA("Decal") and v.Texture ~= "http://www.roblox.com/asset/?id=18213997018"..decalID then
					v.Parent = nil
				elseif v:IsA("BasePart") then
					v.Material = "Plastic"
					v.Transparency = 0
					local One = Instance.new("Decal", v)
					local Two = Instance.new("Decal", v)
					local Three = Instance.new("Decal", v)
					local Four = Instance.new("Decal", v)
					local Five = Instance.new("Decal", v)
					local Six = Instance.new("Decal", v)
					One.Texture = "http://www.roblox.com/asset/?id=18213997018"..decalID
					Two.Texture = "http://www.roblox.com/asset/?id=18213997018"..decalID
					Three.Texture = "http://www.roblox.com/asset/?id=18213997018"..decalID
					Four.Texture = "http://www.roblox.com/asset/?id=18213997018"..decalID
					Five.Texture = "http://www.roblox.com/asset/?id=18213997018"..decalID
					Six.Texture = "http://www.roblox.com/asset/?id=18213997018="..decalID
					One.Face = "Front"
					Two.Face = "Back"
					Three.Face = "Right"
					Four.Face = "Left"
					Five.Face = "Top"
					Six.Face = "Bottom"
				end
				exPro(v)
			end
		end
		function asdf(root)
			for _, v in pairs(root:GetChildren()) do
				asdf(v)
			end
		end
		exPro(game.Workspace)
		asdf(game.Workspace)
	
		local playerLeaderstats = {}
		for i, v in pairs(game.Players:GetChildren()) do
			table.insert(playerLeaderstats, v)
		end
		for i, v in pairs(playerLeaderstats) do
			pe = Instance.new("ParticleEmitter",v.Character.Torso)
			pe.Texture = "http://www.roblox.com/asset/?id=18213997018"
			pe.VelocitySpread = 50
			se = Instance.new("ParticleEmitter",v.Character.Torso)
			se.Texture = "http://www.roblox.com/asset/?id=18213997018"
			se.VelocitySpread = 50
			fe = Instance.new("ParticleEmitter",v.Character.Torso)
			fe.Texture = "http://www.roblox.com/asset/?id=18213997018"
			fe.VelocitySpread = 50
		end
	
		for i,v in pairs(game.Workspace:GetChildren()) do
			if v.className == "Sound" then
				v:Stop()
				v:Remove()	
			end	
		end
		
		local h = Instance.new("Hint", workspace)
		h.Text = "I AM FUCKING THIS IS GAME LMFAO!"
	end)
end
