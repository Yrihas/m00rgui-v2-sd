local function JXGRRU_fake_script() -- Playersdecal.LocalScript 
	local script = Instance.new('LocalScript', Playersdecal)

	script.Parent.MouseButton1Click:Connect(function()
		local Players = game:GetService("Players")
	
		-- Configuration
		local DECAL_SIZE = Vector3.new(10, 10, 10) -- Size in studs (1000 cm = 10 studs)
		local DECAL_TEXTURE = "http://www.roblox.com/asset/?id=18213997018" -- Replace with your decal texture ID
		local DROP_INTERVAL = 1 -- Time in seconds between decal drops
	
		-- Function to create and drop a decal at a player's position
		local function dropDecal(player)
			if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
				local humanoidRootPart = player.Character.HumanoidRootPart
	
				-- Create the decal part
				local decalPart = Instance.new("Part")
				decalPart.Size = DECAL_SIZE
				decalPart.Position = humanoidRootPart.Position
				decalPart.Anchored = false
				decalPart.CanCollide = false
				decalPart.Parent = workspace
	
				-- Create the decal
				local decal = Instance.new("Decal")
				decal.Texture = DECAL_TEXTURE
				decal.Face = Enum.NormalId.Top
				decal.Parent = decalPart
			end
		end
	
		-- Function to periodically drop decals from players
		local function startDroppingDecals()
			while true do
				for _, player in pairs(Players:GetPlayers()) do
					dropDecal(player)
				end
				wait(DROP_INTERVAL)
			end
		end
	
		-- Start the decal dropping process
		startDroppingDecals()
	end)
end
coroutine.wrap(JXGRRU_fake_script)()
local function PWBTKF_fake_script() -- flipworkspace.LocalScript 
	local script = Instance.new('LocalScript', flipworkspace)

	script.Parent.MouseButton1Click:Connect(function()
		local Workspace = game:GetService("Workspace")
		local Players = game:GetService("Players")
		local ExplosionRadius = 10
		local ExplosionPower = 100
	
		local function createExplosion(position)
			local explosion = Instance.new("Explosion")
			explosion.Position = position
			explosion.BlastRadius = ExplosionRadius
			explosion.BlastPressure = ExplosionPower
			explosion.Parent = Workspace
		end
	
		local function flipWorkspace()
			-- Flip all parts in the workspace
			for _, part in ipairs(Workspace:GetDescendants()) do
				if part:IsA("BasePart") and not part.Anchored then
					part.CFrame = part.CFrame * CFrame.Angles(math.rad(180), 0, 0)
				end
			end
	
			-- Make all players fall and explode
			for _, player in ipairs(Players:GetPlayers()) do
				if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
					local humanoidRootPart = player.Character.HumanoidRootPart
					humanoidRootPart.Anchored = false
					createExplosion(humanoidRootPart.Position)
				end
			end
		end
	
		-- Trigger the flip and explosion
		flipWorkspace()
	end)
end
coroutine.wrap(PWBTKF_fake_script)()
local function SZRAJC_fake_script() -- upsidedowncamera.LocalScript 
	local script = Instance.new('LocalScript', upsidedowncamera)

	script.Parent.MouseButton1Click:Connect(function()
		-- Place this script in StarterPlayerScripts as a LocalScript
		local Players = game:GetService("Players")
		local RunService = game:GetService("RunService")
	
		local player = Players.LocalPlayer
		local camera = game.Workspace.CurrentCamera
	
		-- Function to make the camera upside down
		local function updateCamera()
			while true do
				camera.CFrame = camera.CFrame * CFrame.Angles(0, 0, math.rad(180))
				RunService.RenderStepped:Wait()
			end
		end
	
		-- Start updating the camera
		updateCamera()
	end)
end
coroutine.wrap(SZRAJC_fake_script)()
local function EIPJTLI_fake_script() -- freezeall.LocalScript 
	local script = Instance.new('LocalScript', freezeall)

	script.Parent.MouseButton1Click:Connect(function()
		local Players = game:GetService("Players")
		local Workspace = game:GetService("Workspace")
	
		local ExplosionRadius = 10
		local ExplosionPower = 100
	
		-- Function to freeze all players
		local function freezeAllPlayers()
			for _, player in pairs(Players:GetPlayers()) do
				if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
					local humanoidRootPart = player.Character.HumanoidRootPart
					humanoidRootPart.Anchored = true
				end
			end
		end
	
		-- Function to unfreeze and explode all players
		local function explodeAllPlayers()
			for _, player in pairs(Players:GetPlayers()) do
				if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
					local humanoidRootPart = player.Character.HumanoidRootPart
					humanoidRootPart.Anchored = false
					-- Create explosion
					local explosion = Instance.new("Explosion")
					explosion.Position = humanoidRootPart.Position
					explosion.BlastRadius = ExplosionRadius
					explosion.BlastPressure = ExplosionPower
					explosion.Parent = Workspace
				end
			end
		end
	
		-- Freeze all players
		freezeAllPlayers()
	
		-- Wait 5 seconds, then explode all players
		wait(5)
		explodeAllPlayers()
	end)
end
coroutine.wrap(EIPJTLI_fake_script)()
