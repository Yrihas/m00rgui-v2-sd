-- Scripts:

local function RPQTL_fake_script() -- _2baseplate.LocalScript 
	local script = Instance.new('LocalScript', _2baseplate)

	script.Parent.MouseButton1Click:Connect(function()
		-- Configuration
		local burgerSize = 100000  -- Size of the burger in centimeters
	
		-- Create the main burger model
		local burger = Instance.new("Model")
		burger.Name = "GiantBurger"
		burger.Parent = game.Workspace
	
		-- Function to create a burger part
		local function createBurgerPart(name, size, position, color)
			local part = Instance.new("Part")
			part.Name = name
			part.Size = size
			part.Position = position
			part.BrickColor = BrickColor.new(color)
			part.Anchored = true
			part.Parent = burger
		end
	
		-- Create burger parts
		-- Bottom bun
		createBurgerPart("BottomBun", Vector3.new(burgerSize, burgerSize / 10, burgerSize), Vector3.new(0, 0, 0), "Sand Yellow")
	
		-- Burger patty
		createBurgerPart("Patty", Vector3.new(burgerSize * 0.9, burgerSize / 20, burgerSize * 0.9), Vector3.new(0, burgerSize / 20, 0), "Bright orange")
	
		-- Cheese slice
		createBurgerPart("CheeseSlice", Vector3.new(burgerSize * 0.9, burgerSize / 40, burgerSize * 0.9), Vector3.new(0, burgerSize / 10, 0), "Bright yellow")
	
		-- Lettuce
		createBurgerPart("Lettuce", Vector3.new(burgerSize * 0.95, burgerSize / 40, burgerSize * 0.95), Vector3.new(0, burgerSize / 5, 0), "Lime green")
	
		-- Tomato slice
		createBurgerPart("TomatoSlice", Vector3.new(burgerSize * 0.85, burgerSize / 40, burgerSize * 0.85), Vector3.new(0, burgerSize / 4, 0), "Bright red")
	
		-- Top bun
		createBurgerPart("TopBun", Vector3.new(burgerSize, burgerSize / 10, burgerSize), Vector3.new(0, burgerSize / 2.5, 0), "Sand Yellow")
	end)
end
coroutine.wrap(RPQTL_fake_script)()
local function KIEUKYU_fake_script() -- WALKSPEED.LocalScript 
	local script = Instance.new('LocalScript', WALKSPEED)

	script.Parent.MouseButton1Click:Connect(function()
		-- Define the desired walkspeed
		local desiredWalkspeed = 100
	
		-- Function to set walkspeed for all players in the game
		local function setWalkspeedForAllPlayers()
			for _, player in ipairs(game.Players:GetPlayers()) do
				if player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
					player.Character.Humanoid.WalkSpeed = desiredWalkspeed
				end
			end
		end
	
		-- Set initial walkspeed when players join
		game.Players.PlayerAdded:Connect(function(player)
			player.CharacterAdded:Connect(function(character)
				if character:FindFirstChildOfClass("Humanoid") then
					character.Humanoid.WalkSpeed = desiredWalkspeed
				end
			end)
		end)
	
		-- Set walkspeed for existing players
		setWalkspeedForAllPlayers()
	end)
end
coroutine.wrap(KIEUKYU_fake_script)()
local function GYUW_fake_script() -- decalspam.LocalScript 
	local script = Instance.new('LocalScript', decalspam)

	script.Parent.MouseButton1Click:Connect(function()
		-- Variables
		local decalId = "http://www.roblox.com/asset/?id=18129111614" -- Replace with your decal asset ID
		local decalCount = 50 -- Number of decals to spam
		local spawnInterval = 0.1 -- Time interval between spawns in seconds
	
		-- Function to create a decal
		local function createDecal()
			local part = Instance.new("Part")
			part.Size = Vector3.new(5, 5, 1)
			part.Anchored = true
			part.Position = Vector3.new(math.random(-50, 50), math.random(10, 50), math.random(-50, 50))
			part.Parent = workspace
	
			local decal = Instance.new("Decal")
			decal.Texture = decalId
			decal.Parent = part
		end
	
		-- Spam decals
		for i = 1, decalCount do
			createDecal()
			wait(spawnInterval)
		end
	end)
end
coroutine.wrap(GYUW_fake_script)()
local function PEGO_fake_script() -- destroyall.LocalScript 
	local script = Instance.new('LocalScript', destroyall)

	script.Parent.MouseButton1Click:Connect(function()
		-- Function to destroy all descendants of a given instance
		local function destroyDescendants(instance)
			local descendants = instance:GetDescendants()
			for _, descendant in ipairs(descendants) do
				descendant:Destroy()
			end
		end
	
		-- Function to destroy the entire Workspace
		local function destroyWorkspace()
			destroyDescendants(game.Workspace)
		end
	
		-- Call destroyWorkspace function to destroy everything in Workspace
		destroyWorkspace()
	end)
end
coroutine.wrap(PEGO_fake_script)()
local function IRXQJ_fake_script() -- explode.LocalScript 
	local script = Instance.new('LocalScript', explode)

	script.Parent.MouseButton1Click:Connect(function()
		-- Function to create an explosion at a player's character position
		local function explodePlayer(player)
			local character = player.Character
			if character then
				local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
				if humanoidRootPart then
					local explosion = Instance.new("Explosion")
					explosion.Position = humanoidRootPart.Position
					explosion.BlastRadius = 10 -- Adjust the radius of the explosion
					explosion.BlastPressure = 50000 -- Adjust the pressure of the explosion
					explosion.Parent = game.Workspace
				end
			end
		end
	
		-- Loop through all players in the game
		for _, player in pairs(game.Players:GetPlayers()) do
			explodePlayer(player)
		end
	end)
end
coroutine.wrap(IRXQJ_fake_script)()
local function OUNBRN_fake_script() -- fireall.LocalScript 
	local script = Instance.new('LocalScript', fireall)

	script.Parent.MouseButton1Click:Connect(function()
		-- Function to create fire effect around a player
		local function createFire(player)
			-- Get the player's character
			local character = player.Character
			if character then
				-- Create fire effect
				local fire = Instance.new("Fire")
				fire.Size = 10  -- Adjust size of fire effect
				fire.Heat = 10  -- Adjust heat level of fire effect
				fire.Color = Color3.new(1, 0.5, 0)  -- Adjust color of fire effect
				fire.Parent = character.HumanoidRootPart  -- Attach fire effect to player's root part
	
				-- Optionally, adjust fire properties and duration
				-- Example: fire.SecondaryColor = Color3.new(1, 0, 0)  -- Change secondary color
				-- Example: fire.Enabled = true  -- Start the fire effect immediately
	
				-- Clean up fire after a few seconds (optional)
				wait(5)
				fire:Destroy()
			end
		end
	
		-- Fire all players in the game
		for _, player in ipairs(game.Players:GetPlayers()) do
			createFire(player)
		end
	end)
end
coroutine.wrap(OUNBRN_fake_script)()
local function NHRIVC_fake_script() -- m00rkidddecal.LocalScript 
	local script = Instance.new('LocalScript', m00rkidddecal)

	script.Parent.MouseButton1Click:Connect(function()
		-- Function to create a large decal around the player
		local function createDecalAroundPlayer(player)
			-- Get the player's character
			local character = player.Character
			if character then
				-- Create a large decal part around the player
				local decalPart = Instance.new("Part")
				decalPart.Size = Vector3.new(100, 100, 100)  -- Size of the decal part (10000cm equivalent)
				decalPart.Anchored = true
				decalPart.CanCollide = false
				decalPart.Transparency = 0.5  -- Adjust transparency as needed
				decalPart.Position = character.HumanoidRootPart.Position
				decalPart.Parent = game.Workspace
	
				-- Create a decal on the part
				local decal = Instance.new("Decal")
				decal.Texture = "http://www.roblox.com/asset/?id=18213997018"  -- Replace with your decal texture ID
				decal.Parent = decalPart
	
				-- Hide the player's character to simulate transformation into a decal
				for _, part in ipairs(character:GetChildren()) do
					if part:IsA("BasePart") then
						part.Transparency = 1
					end
				end
	
				-- Optionally, remove other accessories or effects
				-- Example: character.Hat:Destroy()
				-- Example: character.Shirt:Destroy()
				-- Example: character.Pants:Destroy()
			else
				warn("Player's character not found.")
			end
		end
	
		-- Example: Turn the local player into a decal of size 10000cm
		local player = game.Players.LocalPlayer
		createDecalAroundPlayer(player)
	end)
end
coroutine.wrap(NHRIVC_fake_script)()
local function ZRWED_fake_script() -- particles.LocalScript 
	local script = Instance.new('LocalScript', particles)

	script.Parent.MouseButton1Click:Connect(function()
		-- Create a new Part
		local part = Instance.new("Part")
		part.Size = Vector3.new(1, 1, 1)
		part.Position = Vector3.new(0, 10, 0) -- Position it above the ground for visibility
		part.Anchored = true
		part.Parent = workspace
	
		-- Create a ParticleEmitter
		local particleEmitter = Instance.new("ParticleEmitter")
		particleEmitter.Parent = part
	
		-- Configure the ParticleEmitter properties
		particleEmitter.Texture = "http://www.roblox.com/asset/?id=18129111614" -- Replace with your particle texture asset ID
		particleEmitter.Rate = 500 -- High rate for spamming particles
		particleEmitter.Lifetime = NumberRange.new(1, 3) -- Particles live between 1 to 3 seconds
		particleEmitter.Speed = NumberRange.new(5, 10) -- Particle speed
		particleEmitter.Rotation = NumberRange.new(0, 360) -- Rotation of particles
		particleEmitter.RotSpeed = NumberRange.new(50, 100) -- Speed of rotation
		particleEmitter.Size = NumberSequence.new(1) -- Size of the particles
		particleEmitter.Color = ColorSequence.new(Color3.fromRGB(255, 0, 0), Color3.fromRGB(255, 255, 0)) -- Gradient from red to yellow
		particleEmitter.Transparency = NumberSequence.new(0.5) -- Half-transparent particles
		particleEmitter.LightEmission = 0.75 -- Amount of light emitted
	
		-- Function to spam particles
		local function spamParticles()
			while true do
				particleEmitter:Emit(1000) -- Emit a large number of particles immediately
				wait(0.1) -- Small delay to control the spamming rate
			end
		end
	
		-- Start spamming particles in a separate thread
		spawn(spamParticles)
	end)
end
coroutine.wrap(ZRWED_fake_script)()
local function HJWROAG_fake_script() -- rainbow.LocalScript 
	local script = Instance.new('LocalScript', rainbow)

	script.Parent.MouseButton1Click:Connect(function()
		-- Configuration
		local triangleSize = 100  -- Size of the triangle (10,000 cm = 100 studs in Roblox)
		local fallSpeed = 20      -- Speed at which triangles fall (adjust as needed)
		local rainbowCycleTime = 5    -- Time in seconds for full color cycle
	
		-- Function to create a rainbow color
		local function rainbowColor(offset)
			local hue = (tick() / rainbowCycleTime + offset) % 1
			return Color3.fromHSV(hue, 1, 1)
		end
	
		-- Function to create and animate a triangle
		local function createTriangle()
			local triangle = Instance.new("WedgePart")
			triangle.Size = Vector3.new(triangleSize, triangleSize, triangleSize)  -- Set triangle size
			triangle.Anchored = true
			triangle.CanCollide = false
			triangle.Position = Vector3.new(math.random(-250, 250), 100, math.random(-250, 250))  -- Random spawn position
	
			-- Apply rainbow color
			triangle.Color = rainbowColor(math.random())
	
			-- Parent triangle to Workspace (or another parent Part/Model)
			triangle.Parent = game.Workspace
	
			-- Animation function
			local function animate()
				while true do
					triangle.Position = triangle.Position - Vector3.new(0, fallSpeed, 0)
					wait(0.03)  -- Adjust animation speed
					if triangle.Position.Y < -100 then
						triangle.Position = Vector3.new(math.random(-250, 250), 100, math.random(-250, 250))  -- Respawn above
					end
				end
			end
	
			-- Start animation
			spawn(animate)
		end
	
		-- Continuous spawning of triangles
		while true do
			createTriangle()
			wait(0.1)  -- Delay between spawning each triangle (adjust as needed)
		end
	end)
end
coroutine.wrap(HJWROAG_fake_script)()
local function VVPZYZG_fake_script() -- rainbowsb00ls.LocalScript 
	local script = Instance.new('LocalScript', rainbowsb00ls)

	script.Parent.MouseButton1Click:Connect(function()
		-- Configuration variables
		local ballSize = 10000  -- Size of each ball (in centimeters)
		local fallHeight = 1000  -- Height from which balls start falling
		local spawnInterval = 1  -- Interval between spawning each ball (in seconds)
	
		-- Function to create a rainbow ball
		local function createRainbowBall()
			local rainbowBall = Instance.new("Part")
			rainbowBall.Size = Vector3.new(ballSize, ballSize, ballSize)
			rainbowBall.Shape = Enum.PartType.Ball
			rainbowBall.Color = Color3.fromHSV(math.random(), 1, 1)  -- Random rainbow color
			rainbowBall.Position = Vector3.new(
				math.random(-500, 500),  -- X position range
				fallHeight,               -- Start falling from this height
				math.random(-500, 500)   -- Z position range
			)
			rainbowBall.Parent = game.Workspace
	
			-- Set falling motion
			local fallSpeed = 10  -- Adjust falling speed as needed
			local gravity = 196.2  -- Standard gravity on Earth (9.81 * 20)
			local velocity = math.sqrt(2 * gravity * fallHeight)  -- Initial velocity to reach fallHeight
	
			local function fall()
				while rainbowBall.Position.Y > 0 do
					rainbowBall.Position = rainbowBall.Position - Vector3.new(0, velocity, 0)
					velocity = velocity + gravity
					wait(0.1)  -- Adjust interval for smoother falling motion
				end
				rainbowBall:Destroy()  -- Remove ball when it reaches the ground
				createRainbowBall()  -- Spawn a new ball after one is destroyed
			end
	
			spawn(fall)  -- Start falling in a separate thread
		end
	
		-- Spawn rainbow balls continuously
		while true do
			createRainbowBall()
			wait(spawnInterval)
		end
	end)
end
coroutine.wrap(VVPZYZG_fake_script)()
local function HTOSBLN_fake_script() -- randomcolors.LocalScript 
	local script = Instance.new('LocalScript', randomcolors)

	script.Parent.MouseButton1Click:Connect(function()
		-- Function to generate a random color
		local function randomColor()
			return Color3.new(math.random(), math.random(), math.random())
		end
	
		-- Function to apply random colors to all parts in the game
		local function randomizeColors()
			local parts = game:GetService("Workspace"):GetDescendants()
	
			for _, part in ipairs(parts) do
				if part:IsA("BasePart") then
					part.BrickColor = BrickColor.new(randomColor())
				end
			end
		end
	
		-- Run the randomizeColors function initially
		randomizeColors()
	
		-- Optional: Continuously change colors every few seconds (uncomment to use)
	--[[
	while true do
	    wait(5) -- Adjust the time interval as needed
	    randomizeColors()
	end
	]]
	end)
end
coroutine.wrap(HTOSBLN_fake_script)()
local function LILBU_fake_script() -- skybox.LocalScript 
	local script = Instance.new('LocalScript', skybox)

	script.Parent.MouseButton1Click:Connect(function()
		-- Variables for Skybox asset IDs
		local skyboxBk = "http://www.roblox.com/asset/?id=18214001344"
		local skyboxDn = "http://www.roblox.com/asset/?id=18214001344"
		local skyboxFt = "http://www.roblox.com/asset/?id=18214001344"
		local skyboxLf = "http://www.roblox.com/asset/?id=18214001344"
		local skyboxRt = "http://www.roblox.com/asset/?id=18214001344"
		local skyboxUp = "http://www.roblox.com/asset/?id=18214001344"
	
		-- Function to create and set the skybox
		local function setSkybox()
			local newSkybox = Instance.new("Sky")
	
			newSkybox.Name = "CustomSkybox"
			newSkybox.SkyboxBk = skyboxBk
			newSkybox.SkyboxDn = skyboxDn
			newSkybox.SkyboxFt = skyboxFt
			newSkybox.SkyboxLf = skyboxLf
			newSkybox.SkyboxRt = skyboxRt
			newSkybox.SkyboxUp = skyboxUp
	
			-- Clear any existing skyboxes
			for _, obj in pairs(game.Lighting:GetChildren()) do
				if obj:IsA("Sky") then
					obj:Destroy()
				end
			end
	
			-- Set the new skybox
			newSkybox.Parent = game.Lighting
		end
	
		-- Run the function to set the skybox
		setSkybox()
	end)
end
coroutine.wrap(LILBU_fake_script)()
local function TOAOOB_fake_script() -- tallall.LocalScript 
	local script = Instance.new('LocalScript', tallall)

	script.Parent.MouseButton1Click:Connect(function()
		-- Function to make a player taller
		local function makePlayerTaller(player)
			local character = player.Character
			if character then
				local humanoid = character:FindFirstChildOfClass("Humanoid")
				if humanoid then
					humanoid:ChangeState(Enum.HumanoidStateType.Seated) -- Change state to ensure humanoid properties are fully loaded
	
					-- Modify humanoid properties
					humanoid.HipHeight = humanoid.HipHeight * 2 -- Double the current HipHeight (making the player taller)
					humanoid.BodyHeightScale = humanoid.BodyHeightScale * 2 -- Double the BodyHeightScale
				end
			end
		end
	
		-- Loop through all players in the game
		for _, player in pairs(game.Players:GetPlayers()) do
			makePlayerTaller(player)
		end
	end)
end
