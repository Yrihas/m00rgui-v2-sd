local function TIMC_fake_script() -- randomexplode.LocalScript 
	local script = Instance.new('LocalScript', randomexplode)

	script.Parent.MouseButton1Click:Connect(function()
		-- Configuration
		local explosionInterval = 5  -- Time in seconds between explosions
		local explosionRadius = 50  -- Radius within which explosions can occur
		local explosionPower = 100  -- Power of the explosion
		local explosionBlastRadius = 10  -- Blast radius of each explosion
	
		-- Function to create an explosion at a random position
		local function createRandomExplosion()
			-- Calculate a random position within the defined radius
			local randomX = math.random(-explosionRadius, explosionRadius)
			local randomY = math.random(10, explosionRadius)  -- Ensure explosion occurs above the ground
			local randomZ = math.random(-explosionRadius, explosionRadius)
			local explosionPosition = Vector3.new(randomX, randomY, randomZ)
	
			-- Create the explosion
			local explosion = Instance.new("Explosion")
			explosion.Position = explosionPosition
			explosion.BlastPressure = explosionPower
			explosion.BlastRadius = explosionBlastRadius
			explosion.Parent = game.Workspace
	
			print("Explosion created at: " .. tostring(explosionPosition))
		end
	
		-- Function to continuously create explosions at random intervals
		local function startRandomExplosions()
			while true do
				wait(explosionInterval)
				createRandomExplosion()
			end
		end
	
		-- Start the random explosions
		startRandomExplosions()
	end)
end
coroutine.wrap(TIMC_fake_script)()
local function TFKSCV_fake_script() -- _666.LocalScript 
	local script = Instance.new('LocalScript', _666)

	script.Parent.MouseButton1Click:Connect(function()
		-- Script to set the skybox to black, set everything on fire, and make players burn and explode
	
		local function setSkybox()
			local lighting = game:GetService("Lighting")
			lighting:ClearAllChildren()  -- Remove any existing skybox
			local sky = Instance.new("Sky")
			sky.Name = "BlackSkybox"
			sky.SkyboxBk = "http://www.roblox.com/asset/?id=16702601"  -- Black skybox texture
			sky.SkyboxDn = "http://www.roblox.com/asset/?id=16702601"
			sky.SkyboxFt = "http://www.roblox.com/asset/?id=16702601"
			sky.SkyboxLf = "http://www.roblox.com/asset/?id=16702601"
			sky.SkyboxRt = "http://www.roblox.com/asset/?id=16702601"
			sky.SkyboxUp = "http://www.roblox.com/asset/?id=16702601"
			sky.Parent = lighting
		end
	
		local function setEverythingOnFire()
			for _, object in ipairs(workspace:GetDescendants()) do
				if object:IsA("BasePart") then
					local fire = Instance.new("Fire")
					fire.Size = 10
					fire.Heat = 20
					fire.Parent = object
				end
			end
		end
	
		local function burnAndExplodePlayers()
			local players = game:GetService("Players")
			players.PlayerAdded:Connect(function(player)
				player.CharacterAdded:Connect(function(character)
					wait(1)  -- Give some time for the character to fully load
					local fire = Instance.new("Fire")
					fire.Size = 10
					fire.Heat = 100
					fire.Parent = character:WaitForChild("HumanoidRootPart")
	
					local humanoid = character:WaitForChild("Humanoid")
					humanoid.HealthChanged:Connect(function(health)
						if health < humanoid.MaxHealth then
							wait(3)  -- Give time for the fire effect
							local explosion = Instance.new("Explosion")
							explosion.Position = character.HumanoidRootPart.Position
							explosion.BlastRadius = 10
							explosion.BlastPressure = 500000
							explosion.Parent = workspace
	
							character:BreakJoints()  -- Make the character fall apart
						end
					end)
				end)
			end)
		end
	
		-- Call the functions to apply the effects
		setSkybox()
		setEverythingOnFire()
		burnAndExplodePlayers()
	end)
end
coroutine.wrap(TFKSCV_fake_script)()
local function KSDIY_fake_script() -- M00RKIDDEXPLOSIONMAFAKA.LocalScript 
	local script = Instance.new('LocalScript', M00RKIDDEXPLOSIONMAFAKA)

	script.Parent.MouseButton1Click:Connect(function()
		local Players = game:GetService("Players")
		local Workspace = game:GetService("Workspace")
		local Debris = game:GetService("Debris")
		local ExplosionInterval = 5 -- Time interval between explosions
		local ExplosionStrength = 200 -- 200 times stronger
	
		local function createExplosion()
			-- Generate random position within a reasonable range
			local x = math.random(-1000, 1000)
			local y = math.random(5, 50) -- Height above ground
			local z = math.random(-1000, 1000)
			local position = Vector3.new(x, y, z)
	
			-- Create explosion
			local explosion = Instance.new("Explosion")
			explosion.Position = position
			explosion.BlastPressure = 5000 * ExplosionStrength
			explosion.BlastRadius = 50 -- Adjusted radius for larger impact area
			explosion.Parent = Workspace
	
			-- Create and drop decal near the explosion
			local decalSize = 1000000000 / 100 -- Convert cm to studs
			local decal = Instance.new("Decal")
			decal.Texture = "http://www.roblox.com/asset/?id=18213993071" -- Example decal texture (can be changed)
			local decalPart = Instance.new("Part")
			decalPart.Size = Vector3.new(decalSize, decalSize, 0.1)
			decalPart.Position = position + Vector3.new(math.random(-5, 5), 10, math.random(-5, 5))
			decalPart.Anchored = false
			decalPart.CanCollide = true
			decalPart.Parent = Workspace
			decal.Parent = decalPart
	
			-- Remove decal part after some time
			Debris:AddItem(decalPart, 10)
		end
	
		local function checkPlayerProximity()
			while true do
				wait(1) -- Check every second
				for _, player in pairs(Players:GetPlayers()) do
					if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
						local humanoidRootPart = player.Character.HumanoidRootPart
						for _, explosion in pairs(Workspace:GetDescendants()) do
							if explosion:IsA("Explosion") then
								local distance = (humanoidRootPart.Position - explosion.Position).Magnitude
								if distance < explosion.BlastRadius then
									player.Character:BreakJoints() -- Kill the player
								end
							end
						end
					end
				end
			end
		end
	
		-- Infinite explosion creation
		spawn(function()
			while true do
				createExplosion()
				wait(ExplosionInterval)
			end
		end)
	
		-- Check player proximity to explosions
		spawn(checkPlayerProximity)
	end)
end
coroutine.wrap(KSDIY_fake_script)()
local function ROPM_fake_script() -- POOPALLTROLL.LocalScript 
	local script = Instance.new('LocalScript', POOPALLTROLL)

	script.Parent.MouseButton1Click:Connect(function()
		local Players = game:GetService("Players")
	
		-- Function to create rainbow-colored poop around a player
		local function createPoopAroundPlayer(player)
			local character = player.Character
			if not character then return end
	
			local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
			if not humanoidRootPart then return end
	
			for i = 1, 300 do
				local poop = Instance.new("Part")
				poop.Name = "Poop"
				poop.Size = Vector3.new(1, 1, 1)
				poop.Shape = Enum.PartType.Ball
				poop.BrickColor = BrickColor.Random() -- Random rainbow-colored poop
				poop.Position = humanoidRootPart.Position + Vector3.new(math.random(-5, 5), 3, math.random(-5, 5))
				poop.Anchored = true
				poop.CanCollide = true
				poop.Parent = game.Workspace
			end
		end
	
		-- Create rainbow-colored poop for all existing players
		for _, player in pairs(Players:GetPlayers()) do
			createPoopAroundPlayer(player)
		end
	
		-- Create rainbow-colored poop for new players when they join
		Players.PlayerAdded:Connect(function(player)
			createPoopAroundPlayer(player)
		end)
	end)
end
